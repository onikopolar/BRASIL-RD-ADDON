{
  "name": "brasil-rd-addon",
  "version": "1.0.0",
  "description": "Addon brasileiro para Stremio com suporte completo ao Real-Debrid",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "node build.js",
    "start": "npm run start:stremio",
    "start:stremio": "node dist/server.js",
    "dev": "npm run dev:stremio",
    "dev:stremio": "node --loader ts-node/esm src/server.ts",
    "test": "jest",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "axios": "^1.6.0",
    "cheerio": "^1.1.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.30.0",
    "sequelize": "^6.37.7",
    "stremio-addon-sdk": "^1.6.10",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/puppeteer": "^5.4.6",
    "@types/stremio-addon-sdk": "^1.6.12",
    "jest": "^29.0.0"
  },
  "keywords": [
    "stremio",
    "addon",
    "real-debrid",
    "brasil",
    "streaming",
    "content"
