    "type": "git",
    "url": "https://github.com/brasil-rd/brasil-rd-addon"
  },
  "bugs": {
    "url": "https://github.com/brasil-rd/brasil-rd-addon/issues"
  },
  "homepage": "https://github.com/brasil-rd/brasil-rd-addon#readme",
  "engines": {
    "node": ">=18.0.0"
  }
